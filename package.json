{
  "name": "youtube-speed-reader",
  "description": "Scan youtube videos quickly using captions.",
  "version": "0.0.1",
  "scripts": {
    "dev": "better-npm-run dev",
    "prod": "better-npm-run prod",
    "clean": "rimraf dist",
    "build": "npm run clean && better-npm-run build",
    "test": "better-npm-run test",
    "test:watch": "npm run test -- -watch"
  },
  "betterScripts": {
    "dev": {
      "command": "node server.babel",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "node server.babel",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.prod.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive test",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "author": "Jonathan Shih",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.3.4",
    "better-npm-run": "0.0.8",
    "classnames": "^2.2.3",
    "express": "^4.13.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.6.1",
    "morgan": "^1.7.0",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-bootstrap": "^0.20.1",
    "react-router-redux": "^4.0.0",
    "react-youtube": "^5.1.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.2",
    "xml2json": "^0.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.14.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "nock": "^7.7.2",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-mock-store": "^1.0.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
